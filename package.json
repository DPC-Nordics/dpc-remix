{
  "name": "dpc-remix",
  "private": true,
  "description": "Example app built with Remix.run framework",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"remix dev\" \"npm:db\"",
    "build": "remix build",
    "start": "remix-serve build",
    "db": "json-server --watch db.json --port 4000"
  },
  "dependencies": {
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "json-server": "^0.17.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
